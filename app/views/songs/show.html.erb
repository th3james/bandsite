
<p>
  <strong>Name:</strong>
  <%=h @song.name %>
</p>
<p>
  <strong>Url:</strong>
  <%=h @song.url %>
</p>
<p>
  <strong>Order:</strong>
  <%=h @song.order_int %>
</p>

<p>

  <% if can? :edit, song %>
      <%= link_to "Edit", edit_song_path(@song) %> |
      <%= link_to "Destroy", @song, :confirm => 'Are you sure?', :method => :delete %> |
  <% end %>
  <%= link_to "View All", songs_path %>
</p>
